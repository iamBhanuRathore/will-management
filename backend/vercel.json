{
  "version": 2,
  "builds": [
    {
      "src": "dist/vercel-entry.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/vercel-entry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/vercel-entry.js"
    }
  ],
  "functions": {
    "dist/vercel-entry.js": {
      "runtime": "nodejs20.x"
    }
  },
  "outputDirectory": "dist",
  "installCommand": "npm run vercel-install",
  "buildCommand": "npm run build && npm run vercel-build"
}
